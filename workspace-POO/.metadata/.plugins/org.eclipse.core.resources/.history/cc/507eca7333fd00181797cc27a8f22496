package tp7;

public class Main {

	public static void main(String[] args) {
		
		Com1 c1 = new Com1();
		Com2 c2 = new Com2();
		Liste l = new Liste();
		l.setCommand(c1, c2);
		l.inserer(1);
		l.inserer(2);
		l.inserer(3);
		l.inserer(5);
		l.inserer(7);
		l.inserer(10);
		
	
		l.afficher();
		
		System.out.println(c2.pile);
		
		System.out.println("----------------------Pattern Adaptater---------------------");
		
		Pile<Integer> p = new Pile<>();
		
		p.push(1);
		p.push(2);
		p.push(3);
		p.push(4);
		
		System.out.println(p);
		
		System.out.println("La tÃªte de pile = " + p.peek());
		
		
		

	}

}
